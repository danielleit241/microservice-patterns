      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5000
        }
      ],
      "DownstreamPathTemplate": "/api/items/{id}"
    },
    {
      "UpstreamPathTemplate": "/api/inventory/items/{userId}",
      "UpstreamHttpMethod": [ "GET" ],

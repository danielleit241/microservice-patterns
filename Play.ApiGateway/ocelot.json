        {
          "Host": "localhost",
          "Port": 5000
        }
      ],
      "DownstreamPathTemplate": "/api/items"
    },
    {
      "UpstreamPathTemplate": "/api/items/{id}",
      "UpstreamHttpMethod": [ "GET", "PUT", "DELETE" ],

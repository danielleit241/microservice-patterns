        {
          "Host": "localhost",
          "Port": 5004
        }
      ],
      "DownstreamPathTemplate": "/api/inventory/items/{userId}"
    },
    {
      "UpstreamPathTemplate": "/api/inventory/items",
      "UpstreamHttpMethod": [ "POST" ],
      "DownstreamScheme": "http",
